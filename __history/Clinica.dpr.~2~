program Clinica;

uses
  Vcl.Forms,
  unitprincipal in 'unitprincipal.pas' {Form1},
  unitcadPaciente in 'unitcadPaciente.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
